{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run Mock Server Invoices-SAPUI5",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/invoices-approuter/node_modules/@sap/html5-repo-mock/index.js",
      "args": [
        "--standalone",
        "/nsSAPUI5/test/mockServer.html",
        "--ui5",
        "{\"version\":\".\"}"
      ],
      "cwd": "${workspaceFolder}/SAPUI5",
      "env": {
        "PORT": "6004",
        "MOCK_LOOKUP_DIRS": "[\"webapp\"]",
        "run.config": "{\"handlerId\":\"ui5_run_config_handler_id\",\"runnableId\":\"/home/user/projects/Invoices/SAPUI5\"}"
      },
      "envFile": "${workspaceFolder}/SAPUI5/.env7",
      "preLaunchTask": "Build Invoices"
    },
    {
      "name": "Run Test Invoices-SAPUI5",
